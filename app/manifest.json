{
  "name": "Share to Notes",
  "version": "0.0.3",
  "manifest_version": 2,
  "description": "Share link to apple notes.",
  "background": {
    "scripts": ["background.js"]
  },
  "icons": {
    "48": "baseline_ios_share_black_48dp.png"
  },
  "browser_action": {
    "default_icon": "baseline_ios_share_black_48dp.png"
  },

  "permissions": [
    "nativeMessaging",
    "activeTab",
    "tabs"
  ]
}
